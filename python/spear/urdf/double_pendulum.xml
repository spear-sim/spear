    <link name="base_link">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0.0 0"/>
            <mass value="100"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="-2 -2 -10"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <material>
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="-2 -2 -2"/>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
        </collision>
    </link>

    <link name="link_0">
        <inertial>
            <origin rpy="0 0 0" xyz="0.5 0 0"/>
            <mass value="1"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0.5 0 0"/>
            <geometry>
                <box size="1 0.4 0.4"/>
            </geometry>
            <material>
                <color rgba="0 1.0 0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0.5 0 0"/>
            <geometry>
                <box size="1 0.4 0.4"/>
            </geometry>
        </collision>
    </link>
    <joint name="joint_0" type="continuous">
        <origin rpy="0 0 0" xyz="1 0 0"/>
        <parent link="base_link"/>
        <child link="link_0"/>
        <axis xyz="0 0 1"/>
        <limit effort="0" velocity="1000"/>
        <dynamics type="torque"/>
    </joint>

    <link name="link_1">
        <inertial>
            <origin rpy="0 0 0" xyz="0.5 0 0"/>
            <mass value="1"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>

INTERIORSIM:
  # LAUNCH_MODE: "running_instance"
  LAUNCH_MODE: "standalone_executable"
  STANDALONE_EXECUTABLE: "/home/rachithp/code/github/interiorsim/code/unreal_projects/RobotProject/dist/LinuxNoEditor/RobotProject.sh"
  TEMP_DIR: "/home/rachithp/code/github/interiorsim/code/unreal_projects/RobotProject/Temp"
  VULKAN_DEVICE_FILES: "/usr/share/vulkan/icd.d/nvidia_icd.json"
  RPC_CLIENT_INTERNAL_TIMEOUT_SECONDS: 30.0
  CUSTOM_COMMAND_LINE_ARGUMENTS:
    - "/home/rachithp/code/github/interiorsim/code/unreal_projects/RobotProject/Plugins/RobotSim/setting/settings.json"

SIMULATION_CONTROLLER:
  AGENT_CONTROLLER_NAME: "OpenBotAgentController"
  TASK_NAME: "PointGoalNavigation"

  VISUALIZER:
    CAMERA_ACTOR_NAME: "PIPCamera_2147482533"
    SET_CAMERA_POSE: False
    CAMERA_POSITION_X: -250.0
    CAMERA_POSITION_Y: -155.0
    CAMERA_POSITION_Z: 200.0
    CAMERA_PITCH: -90.0
    CAMERA_YAW: 0.0
    CAMERA_ROLL: 0.0
  
  OPENBOT_AGENT_CONTROLLER:
    AGENT_ACTOR_NAME: "SimpleVehicle_0"
    GOAL_ACTOR_NAME: "Cube_6"
    OBSERVATION_MODE: "mixed"
    MIXED_MODE:
      OBSERVATION_CAMERA_ACTOR_NAME: "PIPCamera_2147482533" # "PIPCamera_2147482533"
      IMAGE_HEIGHT: 128
      IMAGE_WIDTH: 128
      SMARTPHONE_FOV: 72.0
      MOTION_BLUR_AMMOUNT: 10.0
      MOTION_BLUR_MAX: 10.0
      POST_PROC_BLEND_WEIGHT: 1.0
    PHYSICAL_MODE:
      OBSERVATION: "dist-sin-cos" # or "yaw-x-y"

  POINT_GOAL_NAV_TASK:
    AGENT_ACTOR_NAME: "SimpleVehicle_0"
    GOAL_ACTOR_NAME: "Cube_6"
    RANDOM_SEED: 123
    REWARD:
      HIT_GOAL: 1000.0
      HIT_OBSTACLE: 500.0
      DISTANCE_TO_GOAL_SCALE: 0.00001
    EPISODE_BEGIN:
      AGENT_POSITION_X_MIN: -300.0
      AGENT_POSITION_X_MAX: -220.0
      AGENT_POSITION_Y_MIN: -200.0
      AGENT_POSITION_Y_MAX: -120.0
      AGENT_POSITION_Z: 5.0
      GOAL_POSITION_X_MIN: -300.0
      GOAL_POSITION_X_MAX: -220.0
      GOAL_POSITION_Y_MIN: -200.0
      GOAL_POSITION_Y_MAX: -120.0
      GOAL_POSITION_Z: 5.0
      SPAWN_DISTANCE_THRESHOLD: 100.0
    OBSTACLE_IGNORE_ACTOR_NAMES: ["Floor"]

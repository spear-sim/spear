  <link name="base">
    <visual>
      <geometry>
        <box size="1 1 0.1"/>
      </geometry>
      <material name="metal"/>
    </visual>
    <inertial>
      <mass value="1000"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
